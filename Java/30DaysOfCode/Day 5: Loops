import java.util.Scanner;

class Solution{
    static Scanner sc = new Scanner(System.in);
    static int n = sc.nextInt();
    static void table(int n){
        for(int i = 1; i <=10; i++){
            System.out.println(n+" x "+i+" = "+i*n);
        }
    }

    public static void main(String[] args) {
        table(n);
    }
}
